#!/usr/bin/env python
from distutils.core import setup
from catkin_pkg.python_setup import generate_distutils_setup

d = generate_distutils_setup()
d['packages'] = ['arm_move']
d['package_dir'] = {'': 'src'}

c = generate_distutils_setup()
c['packages'] = ['hardcoded_config']
c['package_dir'] = {'': 'config'}


setup(**d)
setup(**c)
